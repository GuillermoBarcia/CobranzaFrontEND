{
  "name": "proveedores-cloud",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "lint": "eslint 'projects'",
    "test": "ng test",
    "format": "prettier --write \"**/*.{ts,html,scss}\"",

    "start": "ng serve --project=shell --port=4200",

    "cobranzas": "concurrently \"npm run start\" \"ng serve --project=cobranzas --port=4201\"",


    "build:shell": "ng build shell --configuration production",
    "build:cobranzas": "ng build cobranzas --configuration production",


    "build:all": "npm run build:shell && npm run build:cobranzas",
    "watch:shell": "ng build shell --watch --configuration development",
    "watch:cobranzas": "ng build cobranzas --watch --configuration development"

  },
  "private": true,
  "dependencies": {
    "@angular-architects/native-federation": "^19.0.19",
